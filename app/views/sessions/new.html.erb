<h1>Log in</h1>

<form>
  <div class="form-group">
    <%= form_with url: sessions_path do |f| %>
      <%= f.label :email %>
      <%= f.email_field :email, class: 'form-control' %>
      <%= f.label :password %>
      <%= f.password_field :password, class: 'form-control' %>
      <%= f.submit "Log in", class: "btn btn-secondary" %>
      <%= link_to "Sign Up", new_user_path, class: "btn btn-outline-secondary space_left_button" %>
    <% end %>
  </div>
</form>